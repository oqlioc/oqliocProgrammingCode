<title>자바 스크립트 배열</title>
<script language="javascript">

var arrNumber = new Array();

arrNumber[0] = 1;
arrNumber[1] = 2;
arrNumber[2] = 3;
arrNumber[3] = 4;
arrNumber[4] = 5;
arrNumber.push(6);
for(var i = 0; i < 6; i++)
{
	<p>
	document.write(arrNumber[i]);
	</p>
}
</script>
